function renderMsg(data) {
  let html = '';
  for(var a = 0; a < data.length; a++) {
    var item = data[a];
    html += '<div class="msg_item"><div class="info"><span>当前用户： <b>'+item.callTypeDesc+' </b>'+item.otherCode+'</span><span class="time">'+item.time+'</span></div></div>';
  }
  return html;
}
