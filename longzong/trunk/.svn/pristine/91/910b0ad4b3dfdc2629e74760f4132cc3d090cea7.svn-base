.msgwrap{
    background: #354052;
    height:auto;
    padding: 36px 10px 0;
    text-align:center;
    color:#fff;
    line-height: 55px;
    min-width: 200px;
    width:200px;
    float: left;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    visibility: hidden;
}
.box{
    background: #e9eaef;  
    display: flex;
}    
.clearfix {
    zoom: 1;
}
.userhead img{
    width:128px;
    height: 128px;
    background: #e4e8e9;
    border-radius: 50%;
}
.username{
    border-bottom: 1px solid #424d5f;
    font-size: 14px;
    text-align: left;
    // width: 200px;
    margin: auto;
}
.name{
    font-size: 16px;
    text-align: center;
}
.goout{
    display: inline-block;
    width: 1.2em;
    height: 1.2em;
    background: url(../../assets/images/goout.png);
    background-size: 100% 100%;
    margin-right: 1em;
}
.username .btnwrap{
    background: unset;
    border: unset;
    color:#fff;
    display: flex;
    line-height: 1;
    margin: auto;
    cursor: pointer;
}
.leave{
    padding: 106px 0 ;
    text-align: center;
    border-bottom: none;

}
.right-content{
   
    float: left;
    width:100%;
    zoom: 1;
    min-width: 1130px;
    background: #e9eaef;
}
.right_btn{
    background: #fff;
    padding: 25px 0 25px 25px;
    width: 100%;
    button{
       margin-right: 15px;
       border-radius: 0;
       padding: 6px 20px;
    }
}
.blue{
    color:#568eeb;
    font-size: 32px;
    line-height: 60px;
}
.sum{
    text-align: center;
}
.productsum{
    height: 270px;
    padding: 20px 0 0;
}
.cont-wrap{
    margin: 15px;
}
.bgbox{
    border-right: 1px dashed #e9eaec;
}
.thisdata{
    span{
        display: inline-block;
        padding: 3px 10px;
        border: 1px solid #4280e7;
        color:#4280e7;
    }
}
.progress{
    margin: 10px 0;
    div{
        display: inline-block;
        height: 10px;
        float: left;
    }
}
.bluebox{
    width:60%;
    background: #4280e7;
}
.yellowbox{
    width:40%;
    background: #f19d21;
}
