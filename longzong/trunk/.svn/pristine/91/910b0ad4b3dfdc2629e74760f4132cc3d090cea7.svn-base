}
.tablebox{
    margin-top: 30px;
    text-align: center;
    width:100%;

    border-width:0px 0px 0px 1px;
    // border: 1px solid #cbcbcb;
    border-color: #cbcbcb;
    line-height: 34px;
    caption{
        border: 1px solid #cbcbcb;
    }
    th{
        background: #ebf1fd;
        border: 1px solid #cbcbcb;
        border-width:0px 1px 1px 0px;
    }
    td{
        border: 1px solid #cbcbcb;
        border-width:0px 1px 1px 0px;
    }
}
.rightcard{
    margin-left: 15px;
}
.hnwrap{
    display: flex;
    line-height: 60px;
    margin-bottom: 20px;
    .htzs{
        display: inline-block;
        width: 60px;
        height:  60px;
        background: url(../../assets/images/hticon.png);
        background-size: 100% 100%;
        margin-right: 15px;
    }
    .htnumber{
        font-size: 32px;
        color:#4280e7;
    }
    .cpxl{
        display: inline-block;
        width: 60px;
        height:  60px;
        background: url(../../assets/images/dollor.png);
        background-size: 100% 100%;
        margin-right: 15px;
    }
}
.dotwrap{
    margin-bottom: 20px;
    .dottext{
        color:#000;
    }
    .dot{
        color:#d8d8d8;
        letter-spacing: 5px;
    }
    .htnum{
        color: #447eea;
        font-size: 18px;
    }
}
.pox{
    margin-bottom: 15px;
}
.middle-content .pmtj{
    font-size: 18px;
}
.timebox{
    margin-top: 20px;
    margin-left: 20px;
    position: relative;
    bottom: 0;
    display: flex;
    
    div{
        position: relative;
        margin-right: 38px;
        span{
            display: inline-block;
            width: 118px;
            height: 33px;
            text-align: center;
            line-height: 33px;
            border:1px solid #d0d0d0;
        }
        .onbox{
            position: relative;
            z-index: 1;
            background: #fff;

        }
        .downbox{
            position: absolute;
            background: #e8e7ed;
            left: 3px;
            top: 3px;
            z-index: 0;
        
        }
    }
    .jdbox{
        span{
           
            border:1px solid #4d87e8;
            color:#4d87e8;
        }
        .downbox{
            background: #4d87e8;
        }
    }
    
}
.middle-content{
    margin-top: 15px;
}
.table-title{
    display: flex;
    justify-content: space-between;
    margin-bottom: 10px;
    .title_l{
        line-height: 33px;
        font-weight: bold;
    }
    .title_r{
       span{
        width: 118px;
        height: 33px;
        display: inline-block;
        text-align: center;
        line-height: 33px;
        background: #e9eaef;
       
       }
        .table-blue{
            background: #427fe9;
            color:#fff;
            margin-right: 20px;
        }
    }

}
