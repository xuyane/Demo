  .step-container{
    width: 100%;
    padding: 30px 20px 0 20px;
    .step1{
      width:160px;
      height:40px;
      position: relative;
      margin-right:30px;
      display: inline-block;
      font-size:16px;
      font-weight:500;
      text-align:center;
      line-height:40px;
      background:#eee;
      
    }
    .step1:after{
      content: '';
      width: 50px;
      height:40px;
      position: absolute;
      right: -50px;
      display:inline-block;
      border-left: 25px solid #eee;
      border-top: 20px solid transparent;
      border-right: 25px solid transparent;
      border-bottom: 20px solid transparent;
    }
    .step2{
      width:160px;
      height:40px;
      position: relative;
      margin-right:30px;
      display: inline-block;
      font-size:16px;
      font-weight:500;
      text-align:center;
      line-height:40px;
      background:#eee;
    }
    .step2:before{
      content: '';
      width: 50px;
      height:40px;
      position: absolute;
      left: -25px;
      display:inline-block;
      border-left: 25px solid transparent;
      border-top: 20px solid #eee;
      border-right: 25px solid #eee;
      border-bottom: 20px solid #eee;
    }
    .step2:after{
      content: '';
      width: 50px;
      height:40px;
      position: absolute;
      right: -50px;
      display:inline-block;
      border-left: 25px solid #eee;
      border-top: 20px solid transparent;
      border-right: 25px solid transparent;
      border-bottom: 20px solid transparent;
    }
    .active{
      background: #4280e8;
      color: #fff;
    }
  
    .active:after{
      border-left-color: #4280e8;
    }
    .active:before{
      border-top-color: #4280e8;
      border-right-color: #4280e8;
      border-bottom-color: #4280e8;
    }
  }
